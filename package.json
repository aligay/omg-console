{
  "name": "omg-console",
  "version": "1.0.3",
  "description": "a better console.log",
  "homepage": "https://github.com/aligay/omy-console",
  "bugs": "https://github.com/aligay/omy-console/issues",
  "license": "MIT",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "clean": "rimraf {src,test,scripts}/**/*.{js,map,d.ts}",
    "debug": "node --inspect-brk lib/index.js",
    "dev": "npm run lint && npm run clean && tsc -w",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "npm-updater": "^3.0.3",
    "source-map": "^0.7.3",
    "source-map-resolve": "^0.5.2",
    "tslib": "1.x.x"
  },
  "devDependencies": {
    "@types/node": "^11.9.4",
    "rimraf": "*",
    "tslint": "5.x.x",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">=6"
  }
}
